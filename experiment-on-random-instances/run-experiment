#!/bin/bash
for n in $(seq 90 10 100); do
    for p in 5 9; do
        for i in $(seq 1 10); do
            echo "---------------------------------------------------"
            echo $n $p $i
            time ./tw-exact-new < gr/gr-$n-$p-$i.gr | head -n1
            time ./tw-exact-new2 < gr/gr-$n-$p-$i.gr | head -n1
            time ./tw-exact-original < gr/gr-$n-$p-$i.gr | head -n1
        done
    done
done
